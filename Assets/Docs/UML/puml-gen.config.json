    "UnityEngine.GameObject",
    "UnityEngine.Transform",
    "UnityEngine.RectTransform",
    "UnityEngine.Sprite",
    "UnityEngine.Material",
    "UnityEngine.Camera",
    "UnityEngine.Collider",
    "UnityEngine.Texture2D",
    "UnityEngine.MeshRenderer",
    "UnityEngine.UI.*",
    "TMPro.*",
    "UnityEngine.Events.UnityEvent"
  ]
}
